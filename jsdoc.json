{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "exclude": [],
        "include": ["source/lib/"],
        "includePattern": ".js$"
    },
    "templates": {
        "referenceTitle": "Data Mapper",
        "disableSort": false,
        "collapse": true,
        "resources": {
            "Node package": "https://www.npmjs.com/package/jul-data-mapper"
        }
    },
    "opts": {
        "template": "build/jsdoc-template",
        "destination": "docs/",
        "encoding": "utf8",
        "private": false,
        "recurse": true
    }
}
